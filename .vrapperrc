"noremap <C-n> <LEFT>
"noremap <C-e> j
"noremap <C-u> k
"noremap <C-i> <RIGHT> 	
"noremap e d
"noremap E D
"noremap l n
"noremap L N
"noremap s :
"noremap ` ^
"noremap k e
"noremap K E

nnoremap Y y$
nnoremap <S-F8> <C-V>
vnoremap <S-F8> <C-V>

eclipseaction copy org.eclipse.ui.edit.copy
map <C-c> :copy<CR>
eclipseaction InvertCase AnyEdit.invertCase.command
nnoremap g~ viw:InvertCase<cr>
eclipseaction UnderCamelCase AnyEdit.camel.command
nnoremap g- viw:UnderCamelCase<cr>

" Makes the current line empty without entirely deleting it.
"nnoremap dal 0d$

"set shiftwidth=4
"set tabstop=4
"set expandtab
set textwidth=100
set autoindent
set ignorecase
set smartcase
"set clipboard unnamed
"set visualother
"set syncmodifiable=nosync
set incsearch
set hlsearch
set hlscope=window
set startnormalmode
set noexitlinkmode

source .localvrapperrc
