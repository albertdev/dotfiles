file_export_version=3.0
/instance/org.eclipse.jdt.ui/org.eclipse.jdt.ui.text.custom_code_templates=<?xml version\="1.0" encoding\="UTF-8" standalone\="no"?><templates><template autoinsert\="false" context\="catchblock_context" deleted\="false" description\="Code in new catch blocks" enabled\="true" id\="org.eclipse.jdt.ui.text.codetemplates.catchblock" name\="catchblock">// ${todo} Auto-generated catch block\r\nLOGGER.error("Failed to run ${enclosing_method}", ${exception_var});</template></templates>
/instance/org.eclipse.jdt.ui/org.eclipse.jdt.ui.text.custom_templates=<?xml version\="1.0" encoding\="UTF-8" standalone\="no"?><templates><template autoinsert\="true" context\="java" deleted\="false" description\="Adds a complete LOGGER." enabled\="true" name\="makeLog">${\:import(org.slf4j.Logger,org.slf4j.LoggerFactory)}private static final Logger LOGGER \= LoggerFactory.getLogger(${enclosing_type}.class);</template><template autoinsert\="true" context\="java" deleted\="false" description\="Insert trace logging for current function begin" enabled\="true" name\="traceBegin">${\:import(be.btr.lettergen.util.logging.LogUtils)}if (LOGGER.isTraceEnabled()){&\#13;\r\n\tLOGGER.trace(LogUtils.beginMethod("${enclosing_method}").param("${enclosing_method_arguments}",${cursor}).toString());&\#13;\r\n}</template><template autoinsert\="true" context\="java" deleted\="false" description\="Insert trace logging for current function end" enabled\="true" name\="traceEnd">${\:import(be.btr.lettergen.util.logging.LogUtils)}if (LOGGER.isTraceEnabled()){&\#13;\r\n\tLOGGER.trace(LogUtils.endMethod("${enclosing_method}").returns(${cursor}).toString());&\#13;\r\n}</template><template autoinsert\="true" context\="java" deleted\="false" description\="Trace function entry." enabled\="true" name\="bSimpleTrace">LOGGER.trace("-&gt; ${enclosing_method}(${enclosing_method_arguments})", ${cursor});</template><template autoinsert\="true" context\="java" deleted\="false" description\="" enabled\="true" name\="eSimpleTrace">LOGGER.trace("&lt;- ${enclosing_method}\: {}", ${cursor});</template><template autoinsert\="true" context\="java" deleted\="false" description\="" enabled\="true" name\="makeL4JLog">${\:import(org.apache.log4j.Logger)}private static final Logger LOGGER \= Logger.getLogger(${enclosing_type}.class);</template><template autoinsert\="true" context\="java" deleted\="false" description\="Insert an end-of-line string constant." enabled\="true" name\="eol">final String eol \= System.getProperty("line.separator");</template></templates>
